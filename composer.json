{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^9.2",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "mpdf/mpdf": "*",
        "pagevamp/laravel-cloudwatch-logs": "^1.0",
        "php-open-source-saver/jwt-auth": "^1.4",
        "predis/predis": "^1.1",
        "prettus/l5-repository": "^2.8",
        "psr/simple-cache": "^2.0",
        "simplesoftwareio/simple-qrcode": "~2",
        "sentry/sentry-laravel": "^3.1",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-tags": "^4.3",
        "symfony/console": "6.0.*",
        "symfony/deprecation-contracts": "3.0.*",
        "symfony/error-handler": "6.0.*",
        "symfony/event-dispatcher": "6.0.*",
        "symfony/event-dispatcher-contracts": "3.0.*",
        "symfony/finder": "6.0.*",
        "symfony/http-foundation": "6.0.*",
        "symfony/http-kernel": "6.0.*",
        "symfony/mailer": "6.0.*",
        "symfony/mime": "6.0.*",
        "symfony/process": "6.0.*",
        "symfony/routing": "6.0.*",
        "symfony/service-contracts": "3.0.*",
        "symfony/string": "6.0.*",
        "symfony/translation": "6.0.*",
        "symfony/translation-contracts": "3.0.*",
        "symfony/var-dumper": "6.0.*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "deployer/deployer": "^7.0",
        "dietercoopman/laravel-showsql": "^1.3",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpmd/phpmd": "^2.12",
        "phpunit/phpunit": "^9.5.10",
        "rakutentech/laravel-request-docs": "^1.23",
        "spatie/laravel-ignition": "^1.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "files": [
            "app/Helpers/Constants/constants.php",
            "app/Helpers/Constants/api_routes.php",
            "app/Helpers/Constants/admin_routes.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "lint": [
            "./vendor/bin/phpcs"
        ],
        "lint-fix": [
            "./vendor/bin/phpcbf"
        ],
        "mess": [
            "./vendor/bin/phpmd app text phpmd.xml --suffixes php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
