{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "aws/aws-sdk-php": "^3.293",
        "cjmellor/level-up": "^1.1",
        "cloudinary-labs/cloudinary-laravel": "^2.0",
        "guzzlehttp/guzzle": "^7.5",
        "hammerstone/fast-paginate": "^1.0",
        "jorenvanhocht/laravel-share": "^4.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.12",
        "maatwebsite/excel": "^3.1",
        "mckenziearts/laravel-notify": "^2.4",
        "mpdf/mpdf": "*",
        "pagevamp/laravel-cloudwatch-logs": "^1.0",
        "php-open-source-saver/jwt-auth": "^2.1",
        "prettus/l5-repository": "^2.8",
        "psr/simple-cache": "^2.0",
        "pusher/pusher-php-server": "^7.2",
        "ralphjsmit/laravel-seo": "^1.4",
        "rap2hpoutre/fast-excel": "^5.3",
        "sentry/sentry-laravel": "^3.8.2",
        "simplesoftwareio/simple-qrcode": "~2",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-sitemap": "^6.3",
        "spatie/laravel-tags": "^4.5",
        "tymon/jwt-auth": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "deployer/deployer": "^7.0",
        "dietercoopman/laravel-showsql": "^1.3",
        "phpmd/phpmd": "^2.12",
        "rakutentech/laravel-request-docs": "^1.23",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "files": [
            "app/const.php",
            "app/helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "lint": [
            "./vendor/bin/phpcs"
        ],
        "lint-fix": [
            "./vendor/bin/phpcbf"
        ],
        "mess": [
            "./vendor/bin/phpmd app text phpmd.xml --suffixes php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
